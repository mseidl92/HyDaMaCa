FoamFile
{{
	format		ascii;
	class		volVectorField;
	object		U;
}}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions	[0 1 -1 0 0 0 0];

internalField	uniform (0. {velocity:.12f} 0.);

boundaryField
{{
	minX
	{{
		type	slip;
	}}
    
	maxX
	{{
		type	slip;
	}}

	minY
	{{
		type	fixedValue;
		value	uniform (0. {velocity:.12f} 0.);
	}}

	maxY
	{{
		type	zeroGradient;
	}}    

	minZ
	{{
		type	slip;
	}}
    
	maxZ
	{{
		type	slip;
	}}
    
	stlsurface
	{{
		type	fixedValue;
		value	uniform (0 0 0);
	}}
}}


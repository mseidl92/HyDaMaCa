FoamFile
{{
	format		ascii;
	class		volVectorField;
	object		U;
}}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions	[0 1 -1 0 0 0 0];

internalField	uniform ({velocity:.12f} 0. 0.);

boundaryField
{{
	minX
	{{
		type	zeroGradient;
	}}

	maxX
	{{
		type	fixedValue;
		value	uniform ({velocity:.12f} 0. 0.);
	}}

	minY
	{{
		type	slip;
	}}
    
	maxY
	{{
		type	slip;
	}}
    
	minZ
	{{
		type	slip;
	}}
    
	maxZ
	{{
		type	symmetry;
	}}
    
	stlsurface
	{{
		type	fixedValue;
		value	uniform (0 0 0);
	}}
}}


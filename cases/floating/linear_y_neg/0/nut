FoamFile
{
	format		ascii;
	class		volScalarField;
	object		nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions	[0 2 -1 0 0 0 0];

internalField	uniform 0.0001;

boundaryField
{
	minX
	{
		type	nutUSpaldingWallFunction;
		value	$internalField;
	}

	maxX
	{
		type	nutUSpaldingWallFunction;
		value	$internalField;
	}

	minY
	{
		type	calculated;
		value	$internalField;
	}

	maxY
	{
		type	calculated;
		value	$internalField;
	}
    
	minZ
	{
		type	nutUSpaldingWallFunction;
		value	$internalField;
	}

	maxZ
	{
		type	symmetry;
	}
    
	stlsurface
	{
		type	nutUSpaldingWallFunction;
		value	$internalField;
	}
}

